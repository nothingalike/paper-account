# Paper Account Configuration
# Copy this file to .env and adjust the values as needed

# Default slippage rate for market orders (as a decimal, e.g., 0.001 for 0.1%)
PAPER_ACCOUNT_DEFAULT_SLIPPAGE=0.001

# Default spread between bid and ask prices (as a decimal, e.g., 0.0005 for 0.05%)
PAPER_ACCOUNT_DEFAULT_SPREAD=0.0005

# Commission rate for trades (as a decimal, e.g., 0.0025 for 0.25%)
PAPER_ACCOUNT_COMMISSION_RATE=0.0025

# Log level for the library (debug, info, warn, error)
PAPER_ACCOUNT_LOG_LEVEL=info

# Path for data persistence (if enabled)
# PAPER_ACCOUNT_STORAGE_PATH=/path/to/storage
